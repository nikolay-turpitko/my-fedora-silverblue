##############################################################################
# my-debian is a repository, containing my own installation scripts and
# configuration files. If you are not me, you may want to use your own fork
# with r/w access to it. You will not be allowed to commit your changes in
# my repository. Also, if you decide to try my settings, you may need
# to use https:// scheme instead of ssh one.
##############################################################################

[Projects/my-debian]
#checkout = git clone 'https://github.com/nikolay-turpitko/my-debian.git' 'my-debian'
checkout = git clone 'git@github.com:nikolay-turpitko/my-debian.git' 'my-debian'

##############################################################################
# Below are private repositories. You may want to remove or replace settings
# below with your own.
##############################################################################

[.password-store]
checkout = git clone 'git@bitbucket.org:nikolay_turpitko/pass.git' '.password-store'

[Projects/current]
checkout = git clone 'git@bitbucket.org:nikolay_turpitko/current.git' 'current'
chain = true

[Projects/private-dotfiles]
checkout = git clone 'git@bitbucket.org:nikolay_turpitko/private-dotfiles.git' 'private-dotfiles'
